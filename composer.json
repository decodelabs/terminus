{
    "name": "decodelabs/terminus",
    "description": "Simple CLI interactions",
    "type": "library",
    "keywords": ["cli", "terminal"],
    "license": "MIT",
    "authors": [{
        "name": "Tom Wright",
        "email": "tom@inflatablecookie.com"
    }],
    "require": {
        "php": "^7.2",
        "decodelabs/glitch": "^0.15",
        "decodelabs/systemic": "^0.6",
        "decodelabs/atlas": "^0.7",
        "psr/log": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8",
        "phpstan/phpstan": "^0.11.16",
        "decodelabs/phpstan-decodelabs": "^0.2"
    },
    "autoload": {
        "psr-4": {
            "DecodeLabs\\Terminus\\": "src/Terminus"
        },
        "files": [
            "src/global.php"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-develop": "0.5.x-dev"
        }
    }
}
