{
    "name": "decodelabs/terminus",
    "description": "Simple CLI interactions",
    "type": "library",
    "keywords": [ "cli", "terminal" ],
    "license": "MIT",
    "authors": [ {
            "name": "Tom Wright",
            "email": "tom@inflatablecookie.com"
        } ],
    "require": {
        "php": "^8.4",

        "decodelabs/archetype": "^0.3",
        "decodelabs/coercion": "^0.2",
        "decodelabs/deliverance": "^0.2",
        "decodelabs/exceptional": "^0.5",
        "decodelabs/glitch-support": "^0.5",
        "decodelabs/tightrope": "^0.2",
        "decodelabs/veneer": "^0.12",

        "psr/log": "^3"
    },
    "require-dev": {
        "decodelabs/phpstan-decodelabs": "^0.7"
    },
    "autoload": {
        "psr-4": {
            "DecodeLabs\\Terminus\\": "src/Terminus",
            "DecodeLabs\\PHPStan\\": "src/PHPStan"
        },
        "files": [
            "src/Terminus/Context.php"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-develop": "0.11.x-dev"
        },
        "effigy": {
            "exports": [
                "phpstan-extension.neon"
            ]
        },
        "phpstan": {
            "includes": [
                "phpstan-extension.neon"
            ]
        }
    },
    "config": {
        "allow-plugins": {
            "phpstan/extension-installer": true
        }
    },
    "repositories": {
    }
}
